
# Initialize the required LangSmith variables for observability
# See: https://docs.langchain.com/langsmith/observability-quickstart
LANGSMITH_TRACING=true
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=lambda-autotuner-agent

# this is for internet search
TAVILY_API_KEY=your_tavily_api_key_here

# MLflow Tracing Configuration
# See: https://mlflow.org/docs/latest/genai/tracing/
# Databricks MLflow Configuration
MLFLOW_TRACKING_URI=databricks
DATABRICKS_HOST=https://your-databricks-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your_databricks_token_here
# MLflow Experiment Name
MLFLOW_EXPERIMENT_NAME=lambda-autotuner-agent
# Enable MLflow Tracing
MLFLOW_ENABLE_TRACING=true
