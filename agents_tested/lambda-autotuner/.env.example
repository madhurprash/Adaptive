
# Initialize the required LangSmith variables for observability
# See: https://docs.langchain.com/langsmith/observability-quickstart
LANGSMITH_TRACING=true
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=lambda-autotuner-agent

# this is for internet search
TAVILY_API_KEY=your_tavily_api_key_here

# Datadog LLM Observability Configuration
# See: https://docs.datadoghq.com/llm_observability/
# Your Datadog API key (required)
DD_API_KEY=your_datadog_api_key_here
# Your Datadog site (default: datadoghq.com, for EU use datadoghq.eu)
DD_SITE=datadoghq.com
# Application name for LLM Observability (optional, defaults to 'lambda-autotuner')
DD_LLMOBS_ML_APP=lambda-autotuner
